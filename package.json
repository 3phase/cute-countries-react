{
  "name": "cute-countries-react",
  "author": "3phase",
  "version": "2.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/cute-countries-react.esm.js",
  "size-limit": [
    {
      "path": "dist/cute-countries-react.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/cute-countries-react.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/preset-react": "^7.26.3",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-url": "^8.0.2",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "husky": "^9.1.7",
    "postcss": "^8.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^11.2.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2"
  },
  "volta": {
    "node": "20.18.0",
    "npm": "10.8.2"
  }
}
